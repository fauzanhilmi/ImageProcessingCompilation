<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pengcit.fauzanhilmi.imageprocessingcompilation" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".imageequalizer.EqualizerActivity"
            android:label="@string/title_activity_equalizer" >
        </activity>
        <activity
            android:name=".ThresholdActivity"
            android:label="@string/title_activity_threshold" >
        </activity>
        <activity
            android:name=".ChainCodeActivity"
            android:label="@string/title_activity_chain_code" >
        </activity>
        <activity
            android:name=".KodeBelokActivity"
            android:label="@string/title_activity_kode_belok" >
        </activity>
        <activity
            android:name=".KodeTulangActivity"
            android:label="@string/title_activity_kode_tulang" >
        </activity>
        <activity
            android:name=".GridActivity"
            android:label="@string/title_activity_grid" >
        </activity>
    </application>

</manifest>
